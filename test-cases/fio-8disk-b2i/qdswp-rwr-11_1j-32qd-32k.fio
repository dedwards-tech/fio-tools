[global]
thread
group_reporting
direct=1
norandommap=1
refill_buffers
ioengine=libaio
time_based
gtod_reduce=1
runtime=60
bs=32k
numjobs=1
iodepth=32
rw=randrw
rwmixread=0

[_dev_sdb]
filename=/dev/sdb

[_dev_sdc]
filename=/dev/sdc

[_dev_sdd]
filename=/dev/sdd

[_dev_sde]
filename=/dev/sde

[_dev_sdf]
filename=/dev/sdf

[_dev_sdg]
filename=/dev/sdg

[_dev_sdh]
filename=/dev/sdh

[_dev_sdi]
filename=/dev/sdi


